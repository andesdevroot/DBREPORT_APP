<p id="notice"><%= notice %></p>

<h1>Mantencioncorrectivas</h1>

<table>
  <thead>
    <tr>
      <th>Fecha</th>
      <th>Hora inicio</th>
      <th>Hora final</th>
      <th>Especialidad</th>
      <th>Observacion</th>
      <th>Area</th>
      <th>Sector</th>
      <th>Equipo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @mantencioncorrectivas.each do |mantencioncorrectiva| %>
      <tr>
        <td><%= mantencioncorrectiva.fecha %></td>
        <td><%= mantencioncorrectiva.hora_inicio %></td>
        <td><%= mantencioncorrectiva.hora_final %></td>
        <td><%= mantencioncorrectiva.especialidad %></td>
        <td><%= mantencioncorrectiva.observacion %></td>
        <td><%= mantencioncorrectiva.area %></td>
        <td><%= mantencioncorrectiva.sector %></td>
        <td><%= mantencioncorrectiva.equipo %></td>
        <td><%= link_to 'Show', mantencioncorrectiva %></td>
        <td><%= link_to 'Edit', edit_mantencioncorrectiva_path(mantencioncorrectiva) %></td>
        <td><%= link_to 'Destroy', mantencioncorrectiva, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Mantencioncorrectiva', new_mantencioncorrectiva_path %>
